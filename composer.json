{
  "name": "marvinrabe/laravel-graphql-test",
  "description": "Provides you with a simple GraphQL testing trait.",
  "keywords": [
    "php",
    "laravel",
    "graphql",
    "testing"
  ],
  "homepage": "https://github.com/marvinrabe/laravel-graphql-test",
  "license": "MIT",
  "authors": [
    {
      "name": "Marvin Rabe",
      "email": "marvin@rabe.pro",
      "homepage": "https://www.rabe.pro",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/contracts": ">=8.0.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.3",
    "vimeo/psalm": "^3.11"
  },
  "autoload": {
    "psr-4": {
      "MarvinRabe\\LaravelGraphQLTest\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MarvinRabe\\LaravelGraphQLTest\\Tests\\": "tests"
    }
  },
  "scripts": {
    "psalm": "vendor/bin/psalm",
    "test": "vendor/bin/phpunit --colors=always",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
